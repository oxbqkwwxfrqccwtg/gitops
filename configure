printf %s "checking if automake is required... " >&6; }
if test "$ARG_AUTOMAKE" '=' 'yes'; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ARG_AUTOCONF" >&5
printf "%s\n" "$ARG_AUTOCONF" >&6; }

    # Check if Makefile.am exists
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for ./Makefile.am" >&5
printf %s "checking for ./Makefile.am... " >&6; }
    if test -f 'Makefile.am'; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: found" >&5
printf "%s\n" "found" >&6; }
    else
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: not found" >&5
printf "%s\n" "not found" >&6; }


    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: reverting..." >&5
printf "%s\n" "$as_me: reverting..." >&6;}

    for file in $AUTOMAKE_FILES; do
        if ! test -f $file && ! test -d $file; then
            continue
        fi

        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking fate of file './$file'" >&5
printf %s "checking fate of file './$file'... " >&6; }
        match=0
        for _file in $LS_SNAPSHOT; do
            if test "$_file" '=' "$file"; then
                match=1
            fi
        done

        if test $match -eq 0 && test "$ARG_ROLLBACK" '=' 'yes'; then
            { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: orphan (remove)" >&5
printf "%s\n" "orphan (remove)" >&6; }
            rm -r $file
        elif test "$ARG_ROLLBACK" '=' 'no'; then
            { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: keep (no rollback)" >&5
printf "%s\n" "keep (no rollback)" >&6; }
        else
            { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: keep" >&5
printf "%s\n" "keep" >&6; }
        fi
    done


    as_fn_error $? "

            \`Makefile.am\` does not exist, but is required.
        " "$LINENO" 5

    fi

    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for ./Makefile.in" >&5
printf %s "checking for ./Makefile.in... " >&6; }
    if ! test -f 'Makefile.in'; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: not found" >&5
printf "%s\n" "not found" >&6; }
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: executing automake..." >&5
printf "%s\n" "$as_me: executing automake..." >&6;}
        set -e
        automake --add-missing
        set +e
